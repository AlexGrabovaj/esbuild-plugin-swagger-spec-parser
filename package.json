{
  "name": "esbuild-plugin-swagger-spec-parser",
  "version": "0.0.3",
  "description": "An esbuild plugin that parses swagger specs.",
  "keywords": [
    "esbuild",
    "esbuild-plugin",
    "plugin",
    "ejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexGrabovaj/esbuild-plugin-swagger-spec-parser"
  },
  "license": "MIT",
  "author": {
    "name": "Aleksander Grabovaj",
    "email": "aleksander.grabovaj@gmail.com"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/swaggerSpecParser.js",
      "require": "./dist/cjs/swaggerSpecParser.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/swaggerSpecParser.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:types": "cp ./src/custom.d.ts ./dist/",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "esbuild": "0.23.1",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=16"
  }
}